{
  "name": "empire-compost-club",
  "version": "1.0.0",
  "description": "A blog for creative code p5.js.",
  "main": "main.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production npm-run-all clean -p build:*",
    "build:eleventy": "eleventy",
    "build:tailwind": "npx tailwindcss -i ./public/assets/css/input.css -o ./public/assets/css/main.css",
    "clean": "rm -rf public",
    "start": "npm-run-all clean -p build:* -p start:*",
    "start:eleventy": "eleventy --serve --quiet",
    "start:tailwind": "tailwindcss -i ./public/assets/css/input.css -o ./public/assets/css/main.css --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "sarahjdawns",
  "license": "ISC",
  "keywords": [],
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "common-tags": "^1.8.2",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.23",
    "postcss-import": "^15.1.0",
    "postcss-loader": "^7.3.0",
    "tailwindcss": "^3.3.2"
  }
}
